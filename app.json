{
  "name": "gitter-irc-bot",
  "description": "A bot for synchronising messages from gitter and irc",
  "repository": "https://github.com/finnp/gitter-irc-bot",
  "keywords": [
    "gitter",
    "irc",
    "synchronization",
    "bot"
  ],
  "env": {
    "GITTERBOT_APIKEY": "get it from https://developer.gitter.im/apps",
    "GITTERBOT_GITTER_ROOM": "user/room",
    "GITTERBOT_IRC_CHANNEL": "#script-irc",
    "GITTERBOT_IRC_NICK": "Testbot",
    "GITTERBOT_IRC_SERVER": "irc.europnet.org",
    "GITTERBOT_IRC_ADMIN": "Pitch|BNC",
    "GITTERBOT_IRC_OPTS": "{}",
    "HEROKU_URL": "http://botsondage0100.herokuapp.com"
  }
}
